    },
    "xpath": {
      "options": { "grid_columns": 12 },
      "type": "string"
    },
    "regex": {
      "options": { "grid_columns": 12 },
      "type": "string"
    },
    "js": {
      "options": { "grid_columns": 12 },
      "type": "string",
      "format": "javascript",
      "default": "function process(r) {\n  return r;\n}"
    }
  }
}
